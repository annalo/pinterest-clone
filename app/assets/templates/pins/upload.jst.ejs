<h3 align="center">Upload an image</h3>
<form id="upload_pin_form" enctype="multipart/form-data">
  <div class="row">
    <div class="form-group">
      <div class="col-xs-8 col-xs-offset-1">
        <input class="form-control" type="file" name="pins[img]">
      </div>
  
      <div class="col-xs-2">
        <button type="submit" class="btn btn-primary">Submit</button>
      </div>
    </div>
  </div>
</form>
